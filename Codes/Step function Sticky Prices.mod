//////////////////////////////////////////////////////////////////
//   Calculating Liquidity Trap Duration & Average Multiplier   //
//////////////////////////////////////////////////////////////////

// Calculate the Duration of the Liquidity Trap generated by each Shock
liqduration = zeros(size(B,3),1);
for i = 1:size(B,3)
	z = sum(B(3, 1:end, i) == log(ibar));
	liqduration(i) = z;
end;


// Exctract the first Value of rpot_CU of each Simulation
// Calculation for better Understanding of the Impulse Responses
rpot = zeros(size(B,3),1);
for i = 1:size(B,3)
	rpot(i) = B(5, 2, i);
end;


// Plotting Figure to show the Variation of the Consumption Taste Shock as a Function of the Liquidity Trap Duration
figure(1);
plot(m, liqduration, 'b',  'linewidth',5);
title('Variation in Consumption Taste Shock and Liquidity Trap Duration','fontweight','bold','FontSize',20);
set(gca,'fontsize',18);
ylabel('Zero Lower Bound Duration Duration');
xlabel('Preference Shock Variation');
hold off;

//Plotting Figure to show the Liquidity Trap Duration and the Path of the Potential Real Rate
figure(2);
plot(rpot, liqduration, 'b',  'linewidth',5);                                                     //change plot(..) to scatter(..) to get the scatter plot
title('Variation in Potential Real Interest Rate and Liquidity Trap Duration','fontweight','bold','FontSize',20);
set(gca,'fontsize',18);
ylabel('Zero Lower Bound Duration Duration');
xlabel('Potential Real Interest Rate');



//Calculation of Average Output Increase after four Quarter
//Substract Matrix A from Matrix B to get the isolated Effects of the Government Shock
//Government Only = Both Shocks - Taste Shock Only
C = B - A

//Exctract the first four Values of y_D and y_F of each Simulation
yD = zeros(size(C,3), 4);
yF = zeros(size(C,3), 4);
g  = zeros(size(C,3), 4);


for i = 1:size(C,3)
	for j = 1:5
	z = C(13 ,j ,i );
  yD(i,j) = z;

  z = C(23, j, i )
  yF(i,j) = z;

  z = C(21 ,j ,i );
  g(i,j) = z;
	end
end

// Remove the first column because it consists only of zeros
yD (:,1) = [];
yF (:,1) = [];


// Calculate the Average Output Increase after four Quarter for Core & Periphery
avg_yD= zeros(size(C,3),1);
avg_yD= mean(yD,2);

avg_yF= zeros(size(C,3),1);
avg_yF= mean(yF,2);


//Plot Results of the Calculations
figure(3);
subplot(1,2,1);
plot(liqduration, avg_yF, 'b',  'linewidth',2);
set(gca,'fontsize',10);
title('Periphery Output (Average First Year)');
ylabel('Percent (Dev From Baseline)');
xlabel('Zero Lower Bound Duration');
hold off;

subplot(1,2,2);
plot(liqduration, avg_yD, 'b',  'linewidth',2);
set(gca,'fontsize',10);
title('Core Output (Average First Year)');
ylabel('Percent (Dev From Baseline)');
xlabel('Zero Lower Bound Duration');
hold off;


